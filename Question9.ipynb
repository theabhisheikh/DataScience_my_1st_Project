   ],
   "source": [
    "import numpy as np\n",
    "numbers = np.arange(100)\n",
    "\n",
    "even_numbers = numbers[numbers % 2 == 0]\n",
    "\n",
    "first_10_even_numbers = even_numbers[:10]\n",
    "\n",
    "for number in first_10_even_numbers:\n",
    "    print(number)"
   ]
